{
  "name": "screeps-javascript-starter",
  "version": "0.1",
  "description": "A starter project template for the game Screeps",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "push-main": "rollup -c --environment DEST:main",
    "push-pserver": "rollup -c --environment DEST:pserver",
    "push-season": "rollup -c --environment DEST:season",
    "push-sim": "rollup -c --environment DEST:sim",
    "test": "npm run test-unit",
    "test-unit": "jest",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-plocal": "rollup -cw --environment DEST:plocal",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-season": "rollup -cw --environment DEST:season",
    "watch-sim": "rollup -cw --environment DEST:sim"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/lodash": "^3.10.1",
    "@types/screeps": "^3.3.7",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "lodash": "^3.10.1",
    "prettier": "^3.2.5",
    "rollup": "^4.12.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-screeps": "^1.0.1",
    "screeps-jest": "^2.0.2"
  },
  "dependencies": {}
}
